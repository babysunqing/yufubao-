/* pages/gathering/gathering.wxss */
page{
  background-color: #f5f5f5
}
.container{
  padding: 32rpx 32rpx 0 32rpx;
}
.monetContent{
  width: 100%;
  height: 132rpx;
  line-height: 132rpx;
  border: 1px #ccc solid;
  background-color: #fff;
  border-radius: 10px;
}
.monetContent .title{
  width: 75rpx;
  height: 100%;
  color: #999;
  font-size: 36rpx;
  margin-left: 40rpx;
  float: left;
}
input{
  width: 80%;
  height: 100%;
  float: left;
  text-align: right;
  color: #333;
}
.submit{
  width: 100%;
  height: 88rpx;
  margin-top: 96rpx;
  background-color: #BF793A;
  font-size: 34rpx;
  color: #fff;
  text-align: center;
  line-height: 88rpx;
  border-radius: 6px
}
